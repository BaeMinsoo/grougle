<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>	
	<typeAliases>
		<typeAlias type="kh.spring.grougle.position.domain.Position" alias="Position"/>
		<typeAlias type="kh.spring.grougle.chat.domain.ChatMessage" alias="ChatMessage"/>
		<typeAlias type="kh.spring.grougle.chat.domain.ChatRoom" alias="ChatRoom"/>
		<typeAlias type="kh.spring.grougle.chat.domain.ChatRoomJoin" alias="ChatRoomJoin"/>
		<typeAlias type="kh.spring.grougle.employee.domain.Employee" alias="Emp"/>
		<typeAlias type="kh.spring.grougle.board.domain.Board" alias="Board"/>
		<typeAlias type="kh.spring.grougle.attendance.domain.Attendance" alias="Attendance"/>
	</typeAliases>
	 
	<mappers>
		 <mapper resource="mappers/position-mapper.xml"/> 
		 <mapper resource="mappers/chat-mapper.xml"/> 
		 <mapper resource="mappers/employee-mapper.xml"/>
		 <mapper resource="mappers/attendance-mapper.xml"/>
	</mappers>

</configuration>
