<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>	
	<typeAliases>
		<typeAlias type="kh.spring.grougle.position.domain.Position" alias="Position"/>
		<typeAlias type="kh.spring.grougle.chat.domain.Chat" alias="Chat"/>
		<typeAlias type="kh.spring.grougle.employee.domain.Employee" alias="Emp"/>
	</typeAliases>
	 
	<mappers>
		 <mapper resource="mappers/position-mapper.xml"/> 
		 <mapper resource="mappers/chat-mapper.xml"/> 
		 <mapper resource="mappers/Employee-mapper.xml"/>
	</mappers>

</configuration>
